export PYTHONPATH=$(pwd)/..

#combined_plots.pdf   
#plot_seed_metrics.pdf            
#plot_valid_files_percentage.pdf  
#args_error_rate.pdf  
#functions_error_rate.pdf    

cat B1*.metrics > /tmp/metrics

poetry run datacollect seed_metrics /tmp/metrics
cp plot_seed_metrics.pdf /var/git/masterppgi2025/img/

poetry run datacollect combined_plots_percent /tmp/metrics
cp combined_plots.pdf /var/git/masterppgi2025/img/      

poetry run datacollect valid_files_percentage /tmp/metrics                  
cp plot_valid_files_percentage.pdf /var/git/masterppgi2025/img/

poetry run datacollect seed_args_and_funcs  /tmp/metrics
cp args_error_rate.pdf /var/git/masterppgi2025/img/args_error_rate.pdf
cp functions_error_rate.pdf /var/git/masterppgi2025/img/functions_error_rate.pdf

cat B1*.coverage > /tmp/coverage
poetry run datacollect coverage_means /tmp/coverage /tmp/metrics
cp edge_heatmap_median.pdf  /var/git/masterppgi2025/img/
cp coverage_median_median.pdf  /var/git/masterppgi2025/img/
cp instruction_heatmap_median.pdf  /var/git/masterppgi2025/img/


cp plot_seed_metrics.pdf                            /var/git/llm_ase2025/ase/imgs/
cp combined_plots.pdf                               /var/git/llm_ase2025/ase/imgs/    
cp plot_valid_files_percentage.pdf                  /var/git/llm_ase2025/ase/imgs/  
cp args_error_rate.pdf                              /var/git/llm_ase2025/ase/imgs/
cp functions_error_rate.pdf                         /var/git/llm_ase2025/ase/imgs/
cp edge_heatmap_median.pdf                          /var/git/llm_ase2025/ase/imgs/
cp coverage_median_median.pdf                       /var/git/llm_ase2025/ase/imgs/
cp instruction_heatmap_median.pdf                   /var/git/llm_ase2025/ase/imgs/

